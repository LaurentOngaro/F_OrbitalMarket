        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "escape-html": "^1.0.3",
        "fastify": "^4.10.2",
        "http-errors": "^2.0.0",
        "lodash": "^4.17.21",
        "mongodb": "^4.12.1",
        "mongoose": "^6.8.0",
        "node-cron": "^3.0.2",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "7.6.0",
