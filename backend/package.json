    "description": "Orbital Market Backend",
    "license": "none",
    "repository": {
        "type": "git",
        "url": "https://github.com/hugoattal/OrbitalMarket.git"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon -T src/server.ts",
        "clean": "tsc --build --clean",
        "start": "cross-env NODE_ENV=production ts-node src/server.ts",
        "api:unreal": "cross-env NODE_ENV=production ts-node -T src/scrapper/unreal",
        "api:unreal-review": "cross-env NODE_ENV=production ts-node -T src/scrapper/unreal/saveReviews",
        "api:unreal-question": "cross-env NODE_ENV=production ts-node -T src/scrapper/unreal/saveQuestions",
        "manual:update-score": "cross-env NODE_ENV=production ts-node src/scripts/updateScore",
        "test": "cross-env NODE_ENV=test vitest",
        "dev:test": "cross-env NODE_ENV=test vitest --ui"
    },
    "_moduleAliases": {
        "@": "./src"
    },
    "dependencies": {
        "@fastify/middie": "^8.1.0",
        "@fastify/static": "^6.6.0",
        "async-mutex": "^0.4.0",
        "axios": "^1.2.1",
